select * from rooms;

delete from rooms where hotel_id>5;
delete from hotels where id > 5;

select * from rooms where hotel_id=0;

select distinct h.id, h.title, h.street, h.city, h.zip, h.email, h.phone, h.web from rooms r inner join hotels h on h.id = r.hotel_id 
where h.title LIKE '%%';

